    setclipboard("https://pastefy.app/zE6N4a6e/raw")
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "LINK COPIED!",
        Text = "Paste it to any browser!",
        Button1 = "Ok",
        Duration = 5
    })
end)

