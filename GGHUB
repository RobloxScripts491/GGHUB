    local magnitude = tonumber(magnitudeInput.Text)
    if magnitude and magnitude > 0 then
        if stopEarthquake then stopEarthquake() end
        stopEarthquake = startEarthquake(magnitude)
    else
        magnitudeInput.Text = "Invalid Magnitude"
    end
end)

