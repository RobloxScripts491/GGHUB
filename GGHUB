    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Troll Gear Tower Script By Baris", "DarkTheme")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Features")

-- Get All Tool Button
Section:NewButton("Get All Tool", "Teleports to locations and returns", function()
    local player = game.Players.LocalPlayer
    local character = player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        local originalPos = character.HumanoidRootPart.Position
        local locations = {
            Vector3.new(280.33831787109375, 181.375, -14.942472457885742),
            Vector3.new(-26.781801223754883, -40.987643241882324, -46.7821044921875),
            Vector3.new(-327.1382751464844, 4.0000152587890625, -43.7821044921875),
            Vector3.new(86.2972183227539, 4),
            Vector3.new(-1.7550508975982666, 353.9999694824219, 287.1971740722656)
        }
        
        for _, pos in ipairs(locations) do
            character.HumanoidRootPart.CFrame = CFrame.new(pos)
            wait(0.5) -- Adjust wait time as needed
        end
        
        -- Return to original position
        character.HumanoidRootPart.CFrame = CFrame.new(originalPos)
    end
end)

-- Get Admin, Carpet, Lasergun Button
Section:NewButton("Get Admin, Carpet, Lasergun", "Fires server events", function()
    local replicatedStorage = game:GetService("ReplicatedStorage")
    replicatedStorage.CarpetEvent:FireServer()
    replicatedStorage.LazerGunEvent:FireServer()
    replicatedStorage.AdminRankEvent:FireServer()
end)

-- Kill All Button
Section:NewButton("Kill All", "Prepares ;kill commands for all players", function()
    local players = game:GetService("Players")
    local localPlayer = players.LocalPlayer
    
    for _, player in ipairs(players:GetPlayers()) do
        if player ~= localPlayer then -- Skip the local player
            local command = ";kill " .. player.Name
            -- Since we can't open the command bar, we'll simulate command execution
            -- This assumes the game has an admin system that processes these commands
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(command, "All")
            wait(0.1) -- Delay to prevent spam
        end
    end
    
    -- Notify player to open command bar manually if needed
    print("Commands sent as chat messages. Open command bar (/) to execute manually if needed.")
end)

-- Coin Farm Toggle
local coinFarmActive = false
Section:NewToggle("Coin Farm", "Teleports to coin locations", function(state)
    local player = game.Players.LocalPlayer
    coinFarmActive = state
    
    if state then
        spawn(function()
            local timewait = 0.3
            local locations = {
                Vector3.new(-12.434035301208496, 356.4478454589844, 285.68255615234375),
                Vector3.new(282.03692626953125, 182.18545532226562, -7.405041217803955),
                Vector3.new(-35.77463912963867, -39.5596923828125, 138.065185546875),
                Vector3.new(-326.78472900390625, 5.3647918701171875, -51.06169891357422),
                Vector3.new(94.66232299804688, 5.020847797393799, -155.49066162109375)
            }
            
            while coinFarmActive do
                for _, pos in ipairs(locations) do
                    if not coinFarmActive then break end
                    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(pos)
                        wait(timewait)
                    end
                end
            end
        end)
    else
        -- Reset player when toggle is turned off
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.Health = 0
        end
    end
end)
end)



