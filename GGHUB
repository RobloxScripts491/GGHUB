            game.Players.LocalPlayer.PlayerGui.Interface.Main.adminFrame.Active = false
            game.Players.LocalPlayer.PlayerGui.Interface.Main.adminFrame.Visible = false
        end)
    else
        game.Players.LocalPlayer:Kick("Yanlis Anahtar")
    end
end)

