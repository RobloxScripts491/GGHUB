                "FLAG", "MEEP", "FEB25", "NEWYEAR", "SNAKE", "2025", "SNOWFLAKE"
            }
            spawn(function()
                for _, code in ipairs(codes) do
                    local success, err = pcall(function()
                        TFunction:InvokeServer(code)
